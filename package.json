  "devDependencies": {
    "express": "^4.18.2",
    "ts-loader": "^9.5.1",
    "typescript": "5.1.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.0.0",
    "webpack-dev-server": "^4.15.1"
  }
